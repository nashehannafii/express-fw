<div class="container mt-3">
  <div class="row pt-5">
    <div class="col text-center">
      <h2 class="display-4">Daftar Kontak</h2>
      <p class="lead">--------------</p>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">No</th>
            <th scope="col">Nama</th>
            <th scope="col">Nomor HP</th>
            <th scope="col">Aksi</th>
          </tr>
        </thead>
        <tbody>
          <% i=1 
          contacts.forEach(contact => { %>
          <tr>
            <th scope="row"><%= i %> </th>
            <td><%= contact.name %> </td>
            <td><%= contact.phone %> </td>
            <td><a href="/detail" class="btn btn-success badge rounded-pill"><i class="bi bi-info-circle"></i> detail</a></td>
          </tr>
          <% i++ }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>